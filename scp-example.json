{
    "Version": "2012-10-17",
    "Statement": [
         {
             "Sid": "ExampleDenyAllOutsideSEAsia",
             "Effect": "Deny",
             "NotAction": [
                 "a4b:*",
                 "budgets:*",
                 "ce:*",
                 "chime:*",
                 "cloudfront:*",
                 "cur:*",
                 "ecr:*",
                 "globalaccelerator:*",
                 "health:*",
                 "iam:*",
                 "importexport:*",
                 "mobileanalytics:*",
                 "organizations:*",
                 "route53:*",
                 "route53domains:*",
                 "shield:*",
                 "support:*",
                 "trustedadvisor:*",
                 "waf:*",
                 "servicequotas:*",
                 "cloudwatch:*",
                 "wellarchitected:*"
             ],
             "Resource": "*",
             "Condition": {
                 "StringNotEquals": {
                     "aws:RequestedRegion": [
                         "ap-southeast-1",
                         "ap-southeast-2"
                     ]
                 },
                 "ArnNotLike": {
                     "aws:PrincipalARN": [
                        "arn:aws:iam::*:role/lw-iam-*",
                        "arn:aws:iam::*:role/*-laceworkcws*",
                        "arn:aws:iam::*:role/*-laceworkcwsrolesa/LACEWORK-CFG-external"
                     ]
                 }
             }
         }
    ]
}